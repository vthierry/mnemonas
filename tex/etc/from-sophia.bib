@Article{	  soltysik-peck-etal:04,
  author		      = {Soltysik, D. and Peck, K. and White, K. and Crosson, B. and Briggs, R.},
  title			      = {Comparison of hemodynamic response nonlinearity across primary cortical areas},
  journal		      = {Neuroimage},
  year			      = {2004},
  volume		      = {22},
  number		      = {3},
  pages			      = {1117--1127},
  month			      = jul,
  url			      = {{{{{{{{{{{{{http://www.sciencedirect.com/science?_ob=MImg\&_imagekey=B6WNP-4CG0R87-2-P\&_cdi=6968\&_orig=browse\&_coverDate=07%2F31%2F2004\&_sk=999779996\&view=c\&wchp=dGLbVtb-zSkzV\&_acct=C000016487\&_version=1\&_userid=297555\&md5=595f36f02eaf8a19d0a6334c9afe3f43\&ie=f.pdf}}}}}}}}}}}}}
				,
  keywords		      = {BOLD fMRI; Linearity; Stimulus duration; Auditory; Motor; Visual; Neural adaptation },
  owner			      = {nwotawa},
  annote		      = {Hemodynamic responses to auditory and visual stimuli and motor tasks were assessed for the
				nonlinearity of response in each of the respective primary cortices. Five stimulus or task
				durations were used (1, 2, 4, 8, and 16 s), and five male subjects (aged 19 1.9 years) were
				imaged. Two tests of linearity were conducted. The first test consisted of using BOLD responses
				to short stimuli to predict responses to longer stimuli. The second test consisted of fitting
				ideal impulse response functions to the observed responses for each event duration. Both methods
				show that the extent of the nonlinearity varies across cortices. Results for the second method
				indicate that the hemodynamic response is nonlinear for stimuli less than 10 s in the primary
				auditory cortex, nonlinear for tasks less than 7 s in the primary motor cortex, and nonlinear
				for stimuli less than 3 s in the primary visual cortex. In addition, neural adaptation functions
				were characterized that could model the observed nonlinearities.}
}

