@Article{	  paragios-deriche:05,
  author		      = {Nikos Paragios and Rachid Deriche},
  title			      = {Geodesic active regions and level set methods for motion estimation and tracking},
  journal		      = {Computer Vision and Image Understanding},
  year			      = {2005},
  volume		      = {97},
  number		      = {3},
  pages			      = {259--282},
  month			      = mar,
  url			      = {http://authors.elsevier.com/sd/article/S1077314204001213},
  keywords		      = {Tracking; Optical flow; Level set method; Implicit representations; Background subtraction;
				Constant brightness constraint; Robust estimators, Affine motion},
  topic			      = {misc},
  owner			      = {der},
  group			      = {Odyssee},
  annote		      = {Motion analysis in computer vision is a well-studied problem with numerous applications. In
				particular, the tasks of optical flow estimation and tracking are of increasing interest. In
				this paper, we propose a level set approach to address both aspects of motion analysis. Our
				approach relies on the propagation of smooth interfaces to perform tracking while using an
				incremental estimation of the motion models. Implicit representations are used to represent
				moving objects, and capture their motion parameters. Information from different sources like a
				boundary attraction term, a background subtraction component and a visual consistency constraint
				are considered. The EulerLagrange equations within a gradient descent method lead to a flow that
				deforms a set of initial curve towards the object boundaries as well an incremental robust
				estimator of their apparent motion. Partial extension of the proposed framework to address dense
				motion estimation and the case of moving observer is also presented. Promising results
				demonstrate the performance of the method.},
  x-editorial-board	      = {yes},
  x-international-audience    = {yes}
}

@Article{	  paragios-rousson-etal:03,
  author		      = {Nikos Paragios and Mika{\"e}l Rousson and Visvanathan Ramesh},
  title			      = {Non-rigid registration using distance functions},
  journal		      = {cviu},
  year			      = 2003,
  volume		      = 89,
  number		      = {2-3},
  pages			      = {142--165},
  month			      = feb,
  url			      = {{{{{{{{{{{{{http://www.sciencedirect.com/web-editions?_ob=MImg\&_imagekey=B6WCX-484SGWB-3-43\&_cdi=6750\&_orig=browse\&_coverDate=03%2F31%2F2003\&_sk=999109997\&view=c\&wchp=dGLbVtb-lSztW\&_acct=C000016487\&_version=1\&_userid=297555\&md5=928dd27e0bd99897060375a8adab6ac3\&ie=f.pdf}}}}}}}}}}}}}
				,
  keywords		      = {Distance functions; Shape matching; Level set representations; Variational methods; Sum of
				squared differences; Joint registration and learning},
  owner			      = {mrousson},
  annote		      = {This paper deals with the registration of geometric shapes. Our primary contribution is the use
				of a simple and robust shape representation (distance functions) for global-to-local alignment.
				We propose a rigid-invariant variational framework that can deal as well with local non-rigid
				transformations. To this end, the registration map consists of a linear motion model and a local
				deformations field, incrementally recovered. In order to demonstrate the performance of the
				selected representation a simple criterion is considered, the sum of square differences.
				Empirical validation and promising results were obtained on examples that exhibit large global
				motion as well as important local deformations and arbitrary topological changes.}
}

