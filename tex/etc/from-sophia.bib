  organization		      = {ARPA},
  url			      = {ftp://ftp-sop.inria.fr/odyssee/Publications/1994/mundy-huang-etal:94.ps.gz},
  keywords		      = {object recognition},
  owner			      = {crothwel},
  annote		      = {MORSE is an object recognition system, based on geometric invariants of 3D structures taken
				from a single 2D intensity view. The system exploits the geometric constraints inherent in
				object classes such polyhedra, rotational symmetry, bi-lateral symmetry and extruded surfaces.
				Invariants have been used in the past to index many of these classes, but MORSE is designed to
				treat multi-class recognition in a uniform system architecture. The class constraints are also
				used to drive image feature extraction and grouping. }
}

