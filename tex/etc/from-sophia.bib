@article{ermentrout-pascal-etal:01,
Author = {Ermentrout, B and Pascal, M and Gutkin, B},
Title = {The effects of spike frequency adaptation and negative feedback on the
   synchronization of neural oscillators},
Journal = {NEURAL COMPUTATION},
Year = {2001},
Volume = {13},
Number = {6},
Pages = {1285-1310},
Month = {JUN},
Abstract = {There are several different biophysical mechanisms for spike frequency
   adaptation observed in recordings from cortical neurons. The two most
   commonly used in modeling studies are a calcium-dependent potassium
   current I-ahp and a slow voltage-dependent potassium current, I-m. We
   show that both of these have strong effects on the synchronization
   properties of excitatorily coupled neurons. Furthermore, we show that
   the reasons for these effects are different. We show through an
   analysis of some standard models, that the M-current adaptation alters
   the mechanism for repetitive firing, while the afterhyperpolarization
   adaptation works via shunting the incoming synapses. This latter
   mechanism applies with a network that has recurrent inhibition. The
   shunting behavior is captured in a simple two-variable reduced model
   that arises near certain types of bifurcations. A one-dimensional map
   is derived from the simplified model.},
Publisher = {M I T PRESS},
Address = {FIVE CAMBRIDGE CENTER, CAMBRIDGE, MA 02142 USA},
