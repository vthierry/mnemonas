@InProceedings{	  gauclin-papadopoulo:00,
  author		      = {Gauclin, Cyrille and Papadopoulo, Theodore},
  title			      = {Fundamental Matrix estimation driven by Stereo-Correlation},
  crossref		      = {accv:00},
  year			      = {2000},
  url			      = {ftp://ftp-sop.inria.fr/odyssee/Publications/2000/gauclin-papadopoulo:00.ps.gz},
  keywords		      = {Fundamental Matrix, Stereo Correlation, Dense Stereo, Multi-Scale, Rectification},
  topic			      = {misc},
  owner			      = {papadop},
  group			      = {Robotvis},
  annote		      = {Traditional stereo by correlation from uncalibrated images is done in two stages: first the
				epipolar geometry (in the form of a Fundamental matrix) is computed using a sparse set of
				matching points across the two images, then the images are rectified correspondingly and
				correlated in order to get the disparity map. In this article, we propose to close the
				estimation loop, i.e to use the correlation scores as feedback information from the stereo
				procedure back into the estimation of epipolar geometry. This loop allows the refinement of both
				the epipolar geometry and the disparity map. Although the method is not limited to a particular
				correlation technique, this paper also describes a new correlation method based on recursive
				filtering that has the advantage of allowing weighted and multi-resolution correlation without
				requiring prohibitive computing time. The results of this method are illustrated on pairs of
				images of human faces and on Scanning Electron Microscope (SEM) images.},
  x-proceedings		      = {yes},
  x-international-audience    = {yes}
}

