    citeulike-linkout-3 = {http://www.hubmed.org/display.cgi?uids=24093015},
    doi = {10.3389/fnhum.2013.00598},
    issn = {1662-5161},
    journal = {Frontiers in human neuroscience},
    pmcid = {PMC3782702},
