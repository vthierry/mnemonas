@InProceedings{	  lopez-perez-deriche-etal:04,
  author		      = {Lopez-Perez, L. and Deriche, R. and Sochen, N.},
  title			      = {The Beltrami Flow over Triangulated Manifolds},
  pages			      = {135--144},
  editor		      = {Sonka, Milan and Kakadiaris, Ioannis A. and Kybic, Jan},
  booktitle		      = {Computer Vision and Mathematical Methods in Medical and Biomedical Image Analysis: ECCV 2004
				Workshops CVAMIA and MMBIA, Prague, Czech Republic, May 15, 2004, Revised Selected Papers},
  year			      = {2004},
  series		      = {Lecture Notes in Computer Science},
  volume		      = {3117},
  month			      = oct,
  publisher		      = {Springer-Verlag Heidelberg},
  url			      = {{http://www.springerlink.com/link.asp?id}, owner = {der} },
  keywords		      = {Regularization on manifolds, Beltrami Flow, Polyakov action, Harmonic maps, Scalar and Color
				image regularization, Non flat surfaces},
  topic			      = {misc},
  group			      = {Odyssee},
  annote		      = {Abstract In several image processing applications one has to deal with noisy images defined on
				surfaces, like electric impulsions or diffusion tensors on the cortex. We propose a new
				regularization technique for data defined on triangulated surfaces: the Beltrami flow over
				intrinsic manifolds. This technique overcomes the over - smoothing of the L2 and the
				stair-casing effects of the L1 flow for strongly noised images. To do so, we locally estimate
				the differential operators and then perform temporal finite differences. We present the
				implementation for scalar images defined in 2 dimensional manifolds and experimental results.},
  x-proceedings		      = {yes},
  x-international-audience    = {yes}
}

