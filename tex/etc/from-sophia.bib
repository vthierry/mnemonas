@Article{	  biederlack-castelo-branco-etal:06,
  author		      = {Julia Biederlack and Miguel Castelo-Branco and Sergio Neuenschwander and Diek W Wheeler and
				Wolf Singer and Danko Nikoli\'c},
  title			      = {Brightness induction: rate enhancement and neuronal synchronization as complementary codes.},
  journal		      = {Neuron},
  year			      = {2006},
  volume		      = {52},
  number		      = {6},
  pages			      = {1073--1083},
  note			      = {07042},
  url			      = {http://dx.doi.org/10.1016/j.neuron.2006.11.012},
  keywords		      = {Action Potentials; Analysis of Variance; Animals; Cats; Contrast Sensitivity; Dose-Response
				Relationship, Radiation; Light; Neurons; Orientation; Pattern Recognition, Visual; Photic
				Stimulation; Psychophysics; Time Factors; Visual Cortex},
  topic			      = {misc},
  owner			      = { pkornp },
  annote		      = {In cat visual cortex, we investigated with parallel recordings from multiple units the neuronal
				correlates of perceived brightness. The perceived brightness of a center grating was changed by
				varying the orientation or the relative spatial phase of a surrounding grating. Brightness
				enhancement by orientation contrast is associated with an increase of discharge rates of
				responses to the center grating but not with changes in spike synchronization. In contrast, if
				brightness enhancement is induced by phase offset, discharge rates are unchanged but
				synchronization increases between neurons responding to the center grating. The changes in
				synchronization correlate well with changes in perceived brightness that were assessed in
				parallel in human subjects using the same stimuli. These results indicate that in cerebral
				cortex the modulation of synchronicity of responses is used as a mechanism complementary to rate
				changes to enhance the saliency of neuronal responses.}
}

