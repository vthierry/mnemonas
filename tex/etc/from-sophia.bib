@TechReport{	  caruyer-aganj-etal:09,
  author		      = {Caruyer, Emmanuel and Aganj, Iman and Lenglet, Christophe and Sapiro, Guillermo and Deriche,
				Rachid},
  title			      = {Online orientation distribution function reconstruction in constant solid angle and its
