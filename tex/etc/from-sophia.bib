@PhDThesis{	  csurka:96,
  author		      = {Csurka, Gabriella},
  title			      = {Modelisation projective des objets tridimensionnels en vision par ordinateur},
  school		      = {Universit\'e de Nice Sophia Antipolis},
  year			      = {1996},
  month			      = apr,
  url			      = {ftp://ftp-sop.inria.fr/odyssee/Publications/PhDs/csurka:96.ps.gz},
  keywords		      = {uncalibrated stereo vision, epipolar geometry, geometric representation, projective invariants,
				projective reconstruction, uncertainty and covariance matrix},
  owner			      = {lucr},
  annote		      = {This thesis studies the possibility of a projective representation of the three-dimensional
				environment using a pair of views obtained with uncalibrated cameras. To be able to execute
				different tasks as a projective reconstruction or computing projective invariants using a pair
				of uncalibrated cameras, it is primordial to know the epipolar geometry, which is perfectly
				characterized by the fundamental matrix. Consequently, the characterization of the uncertainty
				of the fundamental matrix is very important. Therefore, we were studying the possibility to
				estimate this uncertainty using an statistical approach and an analytical one. Knowing the
				epipolar geometry of a pair of views allows us to compute projective invariants of
				three-dimensional objects using only the information found in the two images. So we made a
				detailed study of projective invariants characterizing the different configurations of points or
				plans and/or lines providing also different methods to compute them from the fundamental matrix
				and image measurements. As an indicative example of the application of these invariants, we
				developed implicit methods to compute three-dimensional coordinates from image measurements
				using projective invariants. We compared these methods with the explicit methods of projective
				reconstruction, which are based on estimating the camera matrices and using them to compute the
				three-dimensional coordinates. }
}

