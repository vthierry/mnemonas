@InProceedings{	  rousson-brox-etal:03b,
  author		      = {Rousson, Mika{\"e}l and Brox, Thomas and Deriche, Rachid},
  title			      = {Segmentation active et non supervis{\'e}e d'images textur{\'e}es bas{\'e}e sur le tenseur de
				structure non-lin{\'e}aire},
  booktitle		      = {TAIMA'2003},
  year			      = {2003},
  url			      = {ftp://ftp-sop.inria.fr/odyssee/Publications/2003/rousson-brox-etal:03b.pdf},
  keywords		      = {Th{\'e}orie des courbes de niveaux, segmentation d'images textur{\'e}es, segmentation
				adaptative d'images, diffusion non-lin{\'e}aire, tenseur de structure},
  topic			      = {misc},
  owner			      = {der},
  group			      = {Odyssee},
  annote		      = {Dans cet article, nous proposons une nouvelle approche pour la segmentation active d'images
				textur{\'e}es. Tout d'abord, nous pr{\'e}sentons une m{\'e}thode d'extraction des
				caract{\'e}ristiques textures de l'image, bas{\'e}e sur une version non-lin{\'e}aire du tenseur
				de structure. Ensuite, un cadre variationnel est d{\'e}fini pour incorporer les diff{\'e}rentes
				caract{\'e}risitiques textures et l'intensit{\'e} de l'image dans un processus de segmentation
				adaptatif et non supervis{\'e}. Contrairement aux approches utilisant un jeu de filtres de Gabor
				pour extraire l'information texture, notre approche fournit naturellement un nombre r{\'e}duit
				de composantes. Ainsi, nous {\'e}vitons l'{\'e}tape d'apprentissage souvent n{\'e}cessaire pour
				obtenir une information texture pertinente. Bas{\'e} sur ces nouvelles composantes, nous
				proposons un processus de segmentation actif et adaptatif o{\`u} les densit{\'e}s de
				probabilit{\'e} {\`a} l'int{\'e}rieur et {\`a} l'ext{\'e}rieur du contour sont estim{\'e}es de
				mani{\`e}re dynamique. Nous pr{\'e}sentons des r{\'e}sultats convaincants sur diverses images
				synth{\'e}tiques et r{\'e}elles.},
  x-proceedings		      = {yes},
  x-international-audience    = {yes}
}

