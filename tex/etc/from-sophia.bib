@InProceedings{	  sturm:98,
  author		      = {P. Sturm},
  title			      = {Camera Self-Calibration: A Case Against Kruppa's Equations},
  pages			      = {172--175},
  booktitle		      = {EEE International Conference on Image Processing},
  year			      = 1998,
  address		      = {Chicago, IL},
  month			      = oct,
  url			      = {ftp://ftp.inrialpes.fr/pub/movi/publications/Sturm-icip98.ps.gz},
  owner			      = {mlouraki},
  annote		      = { In this paper, we consider the self-calibration problem for perspective cameras, and
				especially the best known practical method, the so-called Kruppa equation approach. It is known
				that for several common types of camera motion, self-calibration is degenerate, which manifests
				itself through the existence of ambiguous solutions. In a previous paper, we have studied these
				critical motion sequences in detail and have revealed their importance for practical
				applications. In this paper, we reveal a type of camera motion that is not critical for the
				generic self-calibration problem, but for which the Kruppa equation approach fails. This is the
				case if all optical centers lie on a sphere and if the optical axes pass through the sphere's
				center. This situation is very natural for 3D object modeling from photographs or image
				sequences. Our result is thus a contribution to the understanding of how to successfully apply
				self-calibration in practice. }
}

@InProceedings{	  sturm:99,
  author		      = {P. Sturm},
  title			      = {Critical Motion Sequences for the Self-Calibration of Cameras and Stereo Systems with Variable
				Focal Length},
  pages			      = {63--72},
  booktitle		      = {10th British Machine Vision Conference},
  year			      = 1999,
  address		      = {Nottingham, England},
  month			      = sep,
  url			      = {ftp://ftp.inrialpes.fr/pub/movi/publications/Sturm-bmvc99.ps.gz},
  owner			      = {mlouraki},
  annote		      = {We consider the self-calibration problem for a moving camera whose intrinsic parameters are
				known, except the focal length, which may vary freely across different views. The conditions
				under which the determination of the focal length's values for an image sequence is not
				possible, are derived. These depend only on the camera's motions. We give a complete catalogue
				of the so-called critical motion sequences. This is then used to derive the critical motion
				sequences for stereo systems with variable focal lengths. }
}

