  pages			      = {3321},
  booktitle		      = {Proceedings ISMRM},
  year			      = {2008},
  volume		      = {16},
  url			      = {ftp://ftp-sop.inria.fr/odyssee/Publications/2008/ghosh-descoteaux-etal:08b.pdf},
  topic			      = {Odyssee},
  owner			      = {papadop},
  group			      = {Odyssee},
  annote		      = {Historically Diffusion MRI started with Diffusion Tensor Imaging (DTI), which boosted the
				development of schemes for estimating positive definite tensors but were limited by their
				inability to detect fiber-crossings. Recent HARDI techniques have overcome that shortcoming with
				a plethora of new reconstruction schemes such as radial basis functions, Spherical Harmonics
				(SH), Higher Order Tensors (HOT), etc. It is appropriate, therefore, to explore HOT while
				leveraging the extensive framework already established for classical DTI. In this work, we
				propose a review and a comparison of the existing methods and an extension to the Riemannian
				framework to the space of 4 th order diffusion tensors.},
  x-proceedings		      = {yes},
  x-international-audience    = {yes}
}

