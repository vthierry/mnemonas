@InProceedings{	  kybic-clerc:03,
  author		      = {J. Kybic and M. Clerc},
  title			      = {Symmetric {BEM} and Multiscale Fast Multipole Method for the {E/MEG} Problem},
  pages			      = {122--124},
  editor		      = {Pizzella, V. and Romani, G. L.},
  booktitle		      = {NFSI 2003: Proceedings of the 4th International Symposium on Noninvasive Functional Source
				Imaging Within the Human Heart and Brain},
  year			      = {2003},
  month			      = sep,
  keywords		      = {EEG, MEG, BEM, general topology, fast multipole method, multiresolution},
  topic			      = {Misc},
  isbn			      = {0939-4990},
  owner			      = { mclerc },
  annote		      = {We use the novel symmetric boundary element method (BEM) for solving the direct E/MEG problem
				--- computation of the electromagnetic field caused by a~known distribution of sources. We
				generalize the classical shell model of the head and reformulate the symmetric BEM for this
				topology. We have derived a variant of the fast multipole method (FMM) for the symmetric BEM.
				Finally, we show how to solve the problem in a multiscale fashion, to reduce the computational
				complexity for large problems.}
}

