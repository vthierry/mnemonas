@PhDThesis{	  salden:96,
  author		      = {Alfons H. Salden},
  title			      = {Dynamic Scale Space Paradigms},
  school		      = {Utrecht University, Medical Faculty},
  year			      = {1996},
  address		      = {Heidelberglaan 100, Utrecht, The Netherlands},
  month			      = nov,
  url			      = {ftp://ftp-sop.inria.fr/odyssee/Publications/PhDs/salden:96.ps.gz},
  keywords		      = {scale space theory, algebraic invariance theory, diferential geometry, integral geometry,
				topology, Cauchy problems},
  isbn			      = {90-393-1436-5},
  owner			      = {asalden},
  annote		      = {On the basis of physical principles a general theory for the analysis of image formation is
				proposed. This analysis is based on local and multi-local methods involving differential and
				integral equations to perform a smoothing of the image formation taking into account various
				"symmetries" such as that induced by the projective group action on the luminance field. Besides
				schemes for performing particular user-defined relaxations of the image formation several
				disciplines are considered to help quantise this formation, namely differential and integral
				geometry, classical invariant theory and topology. One of the outcomes is that in order to
				smooth the image formation the best thing to do is to quantise first it in terms of so-called
				integral invariants read out by slot-machines such as the torsion and curvature two-forms. These
				density fields can subsequently relaxed by a connexion induced by themselves. Another
				possibility would be to start a so to spaek topological relaxation of the image formation by
				considering the ridges, ruts and extrema as the only meaningful physical objetcs in the image
				formation. }
}

