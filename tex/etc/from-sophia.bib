@Article{	  bellettini-coscia:94,
  author		      = {Bellettini, G. and Coscia, A.},
  title			      = {Approximation of a functional depending on jumps and corners},
  journal		      = bolletino,
  year			      = 1994,
  volume		      = 8,
  number		      = 1,
  pages			      = {151--181},
  owner			      = {pkornp},
  annote		      = {Let $g\in L\sp 2(a,b)$ and let $\alpha$ and $\beta$ be two real numbers such that
				$0<\beta\leq\alpha\leq 2\beta$. Let $$G(u)= \int\sp b\sb a\vert\ddot u\vert\sp 2 dt+
				\alpha\#(S\sb u)+ \beta\# (S\sb{\dot u}\backslash S\sb u)+ \int\sp b\sb a\vert u-g\vert\sp 2
				dt,$$ where $u$ belongs to the space ${\cal H}\sp 2(a,b)$ of piecewise $H\sp 2$ functions, $\#
				(S\sb u)$ and $\#(S\sb{\dot u}\backslash S\sb u)$ denote, respectively, the number of jump and
				corner points of $u$: corner points of $u$ are jump points of $\dot u$ which are not jump points
				of $u$. The authors show how to find a variational approximation of $G$ via De Giorgi's
				$\Gamma$-convergence [{\it E. De Giorgi}, Boll. Unione mat. Ital., V. Ser. A 14, 213-220 (1977;
				Zbl. 389.49008)] by an equi-coercive sequence of elliptic functionals which do not depend on
				jumps or corners.\par The minimum problem $\inf\{G(u): u\in {\cal H}\sp 2(a,b)\}$ has recently
				been suggested by {\it J. Shah} [SIAM J. Control Optimization 30, No. 1, 99-111 (1992; Zbl.
				769.68126)] as a variational approximation to the segmentation problem corresponding to the datum $g$.}
}

@Article{	  bellettini-coscia:94b,
  author		      = {Bellettini, G. and Coscia, A.},
  title			      = {Discrete approximation of a free discontinuity problem},
  journal		      = {Numerical Functional Analysis and Optimization},
  year			      = 1994,
  volume		      = 15,
  number		      = {3--4},
  pages			      = {201--224},
  owner			      = {pkornp},
  annote		      = {Authors' summary: We approximate by discrete $\Gamma$-convergence a functional proposed by
				Mumford-Shah for a variational approach to image segmentation. Such a functional is first
				relaxed with a sequence of nonconvex functionals, which in turn, are discretized by piecewise
				linear finite elements. Under a suitable relation between the relaxation parameter $\varepsilon$
				and the mesh size $h$, the convergence of the discrete functionals and the compactness of any
				sequence of discrete minimizers are proved. The proof relies on the techniques of $\Gamma$-
				convergence and on the properties of the Lagrange interpolation and Clement operators}
}

