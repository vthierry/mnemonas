  authorURLs = {http://www.chaos.gwdg.de/nldpeoplefolder.2007-07-17.7548289764/nldperson.2007-12-18.1066199466 and http://www.chaos.gwdg.de/nldpeoplefolder.2007-07-17.7548289764/nldperson.2007-08-08.1731064802 and http://www.chaos.gwdg.de/nldpeoplefolder.2007-07-17.7548289764/nldperson.2007-12-12.4279182948},
  title = {Sequential Desynchronization in Networks of Spiking Neurons with Partial Reset},
  year = {2009},
  URL = {http://link.aps.org/doi/10.1103/PhysRevLett.102.068101},
  abstract = {The response of a neuron to synaptic input strongly depends on whether or not the neuron has just emitted a spike. We propose a neuron model that after spike emission exhibits a partial response to residual input charges and study its collective network dynamics analytically. We uncover a desynchronization mechanism that causes a sequential desynchronization transition: In globally coupled neurons an increase in the strength of the partial response induces a sequence of bifurcations from states with large clusters of synchronously firing neurons, through states with smaller clusters to completely asynchronous spiking. We briefly discuss key consequences of this mechanism for more general networks of biophysical neurons.
},
  journal = {Phys. Rev. Lett.},
  volume = {102},
  pages = {068101},
  note = {Editors Suggestion at Physical Review Letters
+ Free to Read}
}

