    journal = {Network},
    keywords = {neural, processing, retina},
    month = aug,
    number = {3},
    pages = {241--253},
    pmid = {11563528},
    posted-at = {2009-04-06 15:18:59},
    priority = {2},
    title = {{Redundancy reduction revisited.}},
    url = {http://view.ncbi.nlm.nih.gov/pubmed/11563528},
    volume = {12},
