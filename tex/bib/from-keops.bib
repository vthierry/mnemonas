    day = {15},
    doi = {10.1016/j.visres.2009.08.010},
    issn = {1878-5646},
    journal = {Vision research},
    month = oct,
    number = {20},
    pages = {2415--2441},
    pmid = {19683016},
    posted-at = {2012-04-11 12:04:01},
    priority = {3},
    title = {{Microsaccades: small steps on a long way.}},
